# Configuración de PostgreSQL para Prisma
# Reemplaza con tus credenciales reales
DATABASE_URL="postgresql://usuario:contraseña@localhost:5432/inmemso?schema=public"

# Configuración de MongoDB (para la migración)
MONGODB_URI="mongodb://localhost:27017/inmemso"

# Configuración de Payload CMS
PAYLOAD_SECRET="tu-secret-key-cambia-en-producción"

# CRÍTICO PARA VERCEL: Desabilitar prompts interactivos en producción
# Payload usará migraciones precompiladas sin pedir confirmación
PAYLOAD_RUN_MIGRATIONS=false

# Payload CMS REST API
PAYLOAD_API_URL="http://localhost:3000/api"
PAYLOAD_TOKEN=""

# Configuración para desarrollo
NODE_ENV="development"

# Debug (optional)
PAYLOAD_DEBUG=false








